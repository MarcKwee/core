{
  "name": "restapir",
  "version": "2.0.0",
  "description": "",
  "main": "restapir.js",
  "scripts": {
    "test": "mocha -b -t 5s core/test/*.js && xo core",
    "coverage": "nyc mocha -b -t 5s core/test/*.js"
  },
  "author": "",
  "license": "ISC",
  "xo": {
    "esnext": true,
    "space": 2,
    "rules": {
      "space-before-function-paren": 0,
      "max-params": [
        "error",
        5
      ],
      "camelcase": [
        "error",
        {
          "properties": "never"
        }
      ]
    }
  },
  "devDependencies": {
    "mocha": "^5.0.4",
    "nyc": "^11.6.0"
  }
}
